{
    "name": "popphp/pop-archive",
    "description": "Pop Archive Component for PHP Framework",
    "keywords": [
        "archive",
        "zip",
        "tar",
        "rar",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "New BSD",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-zip": "*",
        "ext-bz2": "*",
        "ext-zlib": "*",
        "pear/archive_tar": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },
    "suggest": {
        "ext-rar": "To use the Rar archive adapter"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Archive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Archive\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
